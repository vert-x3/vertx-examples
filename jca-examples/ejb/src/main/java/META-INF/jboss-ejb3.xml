<?xml version="1.1" encoding="UTF-8"?>
<jboss:ejb-jar xmlns:jboss="http://www.jboss.com/xml/ns/javaee"
               xmlns="http://java.sun.com/xml/ns/javaee"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xmlns:r="urn:resource-adapter-binding"
               xsi:schemaLocation="http://www.jboss.com/xml/ns/javaee http://www.jboss.org/j2ee/schema/jboss-ejb3.3.0-SNAPSHOT.xsd http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
               version="3.1"
               impl-version="2.0">
    <assembly-descriptor>
      <r:resource-adapter-binding>
        <ejb-name>*</ejb-name>
        <r:resource-adapter-name>@ear.name@#@rar.file.name@</r:resource-adapter-name>
      </r:resource-adapter-binding>
    </assembly-descriptor>
</jboss:ejb-jar>
